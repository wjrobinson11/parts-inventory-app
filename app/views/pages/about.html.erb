      Copyright © 2017 RFQPARTS.net, All Rights Reserved.<br>
    </div>
  </footer>

  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <script type="application/ld+json">
      {
          "@context": "http://schema.org",
          "@type": "WebSite",
          "name": "rfqparts.net",
          "url": "https://rfqparts.net/"
      }
  </script>
  <script type="application/ld+json">
      {
          "@context": "http://schema.org",
          "@type": "WebSite",
          "url": "https://www.rfqparts.net/",
          "potentialAction": {
              "@type": "SearchAction",
              "target": "https://www.rfqparts.net/products?_search={search_term_string}*",
              "query-input": {
                  "@type": "PropertyValueSpecification",
                  "valueRequired": true,
                  "valueMaxLength": 100,
                  "valueName": "search_term_string"
              }
          }
      } 
  </script>
  <script type="text/javascript">
    $(document).ready(function(){
      $('.hamburger-nav').click(function(){
        $('.top-bar .top-bar-section .right.main-nav').slideToggle();
      });
      $('#rfq-button').click(function(){

        if( !$(this).hasClass('open') ){
          $('#rfq-slider-area').animate({
            marginLeft: "-50%"
          },400);
          $('#form-panel').show();
          $(this)
            .addClass("open")
            .html("Back to product image");
        }else{
          $('#rfq-slider-area').animate({
            marginLeft: "0%"
          },400);
          setTimeout(function(){
            $('#form-panel').hide();
          },400)
          $(this)
            .removeClass("open")
            .html("Request for Quote");
        }       
      });
    });
  </script>
